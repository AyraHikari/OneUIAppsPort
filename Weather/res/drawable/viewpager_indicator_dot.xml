<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval" android:innerRadius="@dimen/insight_page_indicator_dot_size">
            <solid android:color="@color/col_particulars_insight_page_indicator" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="oval" android:innerRadius="@dimen/insight_page_indicator_dot_size">
            <solid android:color="@color/col_particulars_insight_page_indicator_unselect" />
        </shape>
    </item>
</selector>